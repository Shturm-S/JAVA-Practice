package JAVAPractice;

/**
 *
 * @author M S I
 */
public class LatihanVar {
    public static void main (String[] args) {
    
      int jarijari;
      double keliling;
      final double pi=3.14;
      
      jarijari = 70;
      
      keliling= 2*pi*jarijari;
      System.out.println("Keliling Lingkaran = "+keliling);
}
    
    
    
}

